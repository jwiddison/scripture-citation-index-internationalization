            // load needed scripts via yepnope so they aren't blocking
            yepnope({
                load: [
                    "//edge.ldscdn.org/cdn2/csp/ldsorg/ver/scripts/medialoader/lds.mediaLoader1.6.0.min.js",
                    "//edge.ldscdn.org/cdn2/common/scripts/other/handlebars/handlebars1.0.0.min.js"
                ],
                complete:function(){
                    // setup the plugin for audio
                    $('#audio-player').mediaLoader({
                        template:"#media-template-audio",
                        mediaList:mediaList,
                        hashLabel:'listen',
                        audioCss:"//edge.ldscdn.org/cdn2/csp/ldsorg/css/common/audio.css",
                        audioJs:"//edge.ldscdn.org/cdn2/common/scripts/jquery/plugins/jplayer/2.1.0/jplayer.min.js"
                    });
                }
            });
        });
